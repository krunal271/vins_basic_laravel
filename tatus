[31m-		leftStr += renderedParts1[leftI];[m
[31m-	}[m
[31m-[m
[31m-	// Similarly, start at the rightmost side of the formatting string and move left[m
[31m-	for ([m
[31m-		rightI = sameUnits.length - 1;[m
