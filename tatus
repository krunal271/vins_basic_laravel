[31m-[m
[31m-// Builds an enhanced moment from args. When given an existing moment, it clones. When given a[m
[31m-// native Date, or called with no arguments (the current time), the resulting moment will be local.[m
[31m-// Anything else needs to be "parsed" (a string or an array), and will be affected by:[m
[31m-//    parseAsUTC - if there is no zone information, should we parse the input in UTC?[m
[31m-//    parseZone - if there is zone information, should we force the zone of the moment?[m
[31m-function makeMoment(args, parseAsUTC, parseZone) {[m
[31m-	var input = args[0];[m
[31m-	var isSingleString = args.length == 1 && typeof input === 'string';[m
[31m-	var isAmbigTime;[m
[31m-	var isAmbigZone;[m
[31m-	var ambigMatch;[m
[31m-	var mom;[m
[31m-[m
[31m-	if (moment.isMoment(input) || isNativeDate(input) || input === undefined) {[m
[31m-		mom = moment.apply(null, args);[m
[31m-	}[m
[31m-	else { // "parsing" is required[m
[31m-		isAmbigTime = false;[m
[31m-		isAmbigZone = false;[m
[31m-[m
[31m-		if (isSingleString) {[m
[31m-			if (ambigDateOfMonthRegex.test(input)) {[m
[31m-				// accept strings like '2014-05', but convert to the first of the month[m
[31m-				input += '-01';[m
[31m-				args = [ input ]; // for when we pass it on to moment's constructor[m
[31m-				isAmbigTime = true;[m
[31m-				isAmbigZone = true;[m
[31m-			}[m
[31m-			else if ((ambigMatch = ambigTimeOrZoneRegex.exec(input))) {[m
[31m-				isAmbigTime = !ambigMatch[5]; // no time part?[m
[31m-				isAmbigZone = true;[m
[31m-			}[m
[31m-		}[m
[31m-		else if ($.isArray(input)) {[m
[31m-			// arrays have no timezone information, so assume ambiguous zone[m
[31m-			isAmbigZone = true;[m
[31m-		}[m
[31m-		// otherwise, probably a string with a format[m
[31m-[m
[31m-		if (parseAsUTC || isAmbigTime) {[m
[31m-			mom = moment.utc.apply(moment, args);[m
[31m-		}[m
[31m-		else {[m
[31m-			mom = moment.apply(null, args);[m
[31m-		}[m
[31m-[m
[31m-		if (isAmbigTime) {[m
[31m-			mom._ambigTime = true;[m
[31m-			mom._ambigZone = true; // ambiguous time always means ambiguous zone[m
[31m-		}[m
[31m-		else if (parseZone) { // let's record the inputted zone somehow[m
[31m-			if (isAmbigZone) {[m
[31m-				mom._ambigZone = true;[m
[31m-			}[m
[31m-			else if (isSingleString) {[m
[31m-				mom.utcOffset(input); // if not a valid zone, will assign UTC[m
[31m-			}[m
[31m-		}[m
[31m-	}[m
[31m-[m
[31m-	mom._fullCalendar = true; // flag for extended functionality[m
[31m-[m
[31m-	return mom;[m
[31m-}[m
