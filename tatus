[31m-[m
[31m-				// make a boolean hash as to whether the event occurs on each day-of-week[m
[31m-				if ((dow = abstractEvent.dow)) {[m
[31m-					dowHash = {};[m
[31m-					for (i = 0; i < dow.length; i++) {[m
[31m-						dowHash[dow[i]] = true;[m
[31m-					}[m
[31m-				}[m
[31m-[m
[31m-				// iterate through every day in the current range[m
[31m-				date = _rangeStart.clone().stripTime(); // holds the date of the current day[m
[31m-				while (date.isBefore(_rangeEnd)) {[m
[31m-[m
[31m-					if (!dowHash || dowHash[date.day()]) { // if everyday, or this particular day-of-week[m
[31m-[m
[31m-						startTime = abstractEvent.start; // the stored start and end properties are times (Durations)[m
