[31m-			suggestedViewHeight = element.parent().height() - queryToolbarsHeight();[m
[31m-		}[m
[31m-		else {[m
[31m-			suggestedViewHeight = Math.round(content.width() / Math.max(t.options.aspectRatio, .5));[m
[31m-		}[m
[31m-	}[m
