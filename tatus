[31m-[m
[31m-function parseFieldSpecs(input) {[m
[31m-	var specs = [];[m
[31m-	var tokens = [];[m
[31m-	var i, token;[m
[31m-[m
[31m-	if (typeof input === 'string') {[m
[31m-		tokens = input.split(/\s*,\s*/);[m
[31m-	}[m
[31m-	else if (typeof input === 'function') {[m
[31m-		tokens = [ input ];[m
[31m-	}[m
[31m-	else if ($.isArray(input)) {[m
[31m-		tokens = input;[m
[31m-	}[m
[31m-[m
[31m-	for (i = 0; i < tokens.length; i++) {[m
[31m-		token = tokens[i];[m
[31m-[m
[31m-		if (typeof token === 'string') {[m
[31m-			specs.push([m
[31m-				token.charAt(0) == '-' ?[m
[31m-					{ field: token.substring(1), order: -1 } :[m
[31m-					{ field: token, order: 1 }[m
[31m-			);[m
[31m-		}[m
[31m-		else if (typeof token === 'function') {[m
[31m-			specs.push({ func: token });[m
[31m-		}[m
[31m-	}[m
[31m-[m
[31m-	return specs;[m
[31m-}[m
[31m-[m
[31m-[m
[31m-function compareByFieldSpecs(obj1, obj2, fieldSpecs) {[m
[31m-	var i;[m
[31m-	var cmp;[m
[31m-[m
[31m-	for (i = 0; i < fieldSpecs.length; i++) {[m
[31m-		cmp = compareByFieldSpec(obj1, obj2, fieldSpecs[i]);[m
[31m-		if (cmp) {[m
[31m-			return cmp;[m
[31m-		}[m
[31m-	}[m
[31m-[m
[31m-	return 0;[m
[31m-}[m
[31m-[m
[31m-[m
[31m-function compareByFieldSpec(obj1, obj2, fieldSpec) {[m
[31m-	if (fieldSpec.func) {[m
[31m-		return fieldSpec.func(obj1, obj2);[m
[31m-	}[m
[31m-	return flexibleCompare(obj1[fieldSpec.field], obj2[fieldSpec.field]) *[m
[31m-		(fieldSpec.order || 1);[m
[31m-}[m
[31m-[m
[31m-[m
[31m-function flexibleCompare(a, b) {[m
[31m-	if (!a && !b) {[m
[31m-		return 0;[m
[31m-	}[m
[31m-	if (b == null) {[m
[31m-		return -1;[m
[31m-	}[m
[31m-	if (a == null) {[m
[31m-		return 1;[m
[31m-	}[m
