[31m-// Merges an array of objects into a single object.[m
[31m-// The second argument allows for an array of property names who's object values will be merged together.[m
[31m-function mergeProps(propObjs, complexProps) {[m
[31m-	var dest = {};[m
[31m-	var i, name;[m
[31m-	var complexObjs;[m
[31m-	var j, val;[m
[31m-	var props;[m
[31m-[m
[31m-	if (complexProps) {[m
[31m-		for (i = 0; i < complexProps.length; i++) {[m
[31m-			name = complexProps[i];[m
[31m-			complexObjs = [];[m
[31m-[m
[31m-			// collect the trailing object values, stopping when a non-object is discovered[m
[31m-			for (j = propObjs.length - 1; j >= 0; j--) {[m
[31m-				val = propObjs[j][name];[m
[31m-[m
[31m-				if (typeof val === 'object') {[m
[31m-					complexObjs.unshift(val);[m
[31m-				}[m
[31m-				else if (val !== undefined) {[m
[31m-					dest[name] = val; // if there were no objects, this value will be used[m
[31m-					break;[m
[31m-				}[m
[31m-			}[m
[31m-[m
[31m-			// if the trailing values were objects, use the merged value[m
[31m-			if (complexObjs.length) {[m
[31m-				dest[name] = mergeProps(complexObjs);[m
[31m-			}[m
[31m-		}[m
[31m-	}[m
[31m-[m
[31m-	// copy values into the destination, going from last to first[m
[31m-	for (i = propObjs.length - 1; i >= 0; i--) {[m
[31m-		props = propObjs[i];[m
[31m-[m
[31m-		for (name in props) {[m
[31m-			if (!(name in dest)) { // if already assigned by previous props or complex props, don't reassign[m
[31m-				dest[name] = props[name];[m
[31m-			}[m
[31m-		}[m
[31m-	}[m
[31m-[m
[31m-	return dest;[m
[31m-}[m
