[31m-			dateDelta: startDelta,[m
[31m-			durationDelta: durationDelta,[m
[31m-			undo: undoFunc[m
[31m-		};[m
[31m-	}[m
[31m-[m
[31m-[m
[31m-	// Modifies an array of events in the following ways (operations are in order):[m
[31m-	// - clear the event's `end`[m
[31m-	// - convert the event to allDay[m
[31m-	// - add `dateDelta` to the start and end[m
[31m-	// - add `durationDelta` to the event's duration[m
[31m-	// - assign `miscProps` to the event[m
[31m-	//[m
[31m-	// Returns a function that can be called to undo all the operations.[m
[31m-	//[m
[31m-	// TODO: don't use so many closures. possible memory issues when lots of events with same ID.[m
[31m-	//[m
[31m-	function mutateEvents(events, clearEnd, allDay, dateDelta, durationDelta, miscProps) {[m
[31m-		var isAmbigTimezone = t.getIsAmbigTimezone();[m
[31m-		var undoFunctions = [];[m
[31m-[m
[31m-		// normalize zero-length deltas to be null[m
[31m-		if (dateDelta && !dateDelta.valueOf()) { dateDelta = null; }[m
[31m-		if (durationDelta && !durationDelta.valueOf()) { durationDelta = null; }[m
[31m-[m
[31m-		$.each(events, function(i, event) {[m
[31m-			var oldProps;[m
[31m-			var newProps;[m
[31m-[m
[31m-			// build an object holding all the old values, both date-related and misc.[m
[31m-			// for the undo function.[m
[31m-			oldProps = {[m
[31m-				start: event.start.clone(),[m
[31m-				end: event.end ? event.end.clone() : null,[m
[31m-				allDay: event.allDay[m
[31m-			};[m
[31m-			$.each(miscProps, function(name) {[m
[31m-				oldProps[name] = event[name];[m
[31m-			});[m
[31m-[m
[31m-			// new date-related properties. work off the original date snapshot.[m
[31m-			// ok to use references because they will be thrown away when backupEventDates is called.[m
[31m-			newProps = {[m
[31m-				start: event._start,[m
[31m-				end: event._end,[m
[31m-				allDay: allDay // normalize the dates in the same regard as the new properties[m
[31m-			};[m
[31m-			normalizeEventDates(newProps); // massages start/end/allDay[m
[31m-[m
[31m-			// strip or ensure the end date[m
[31m-			if (clearEnd) {[m
[31m-				newProps.end = null;[m
[31m-			}[m
[31m-			else if (durationDelta && !newProps.end) { // the duration translation requires an end date[m
[31m-				newProps.end = t.getDefaultEventEnd(newProps.allDay, newProps.start);[m
[31m-			}[m
