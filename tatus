[31m-[m
[31m-// Returns a method bound to the given object context.[m
[31m-// Just like one of the jQuery.proxy signatures, but without the undesired behavior of treating the same method with[m
[31m-// different contexts as identical when binding/unbinding events.[m
[31m-function proxy(obj, methodName) {[m
[31m-	var method = obj[methodName];[m
[31m-[m
[31m-	return function() {[m
[31m-		return method.apply(obj, arguments);[m
[31m-	};[m
[31m-}[m
[31m-[m
[31m-[m
[31m-// Returns a function, that, as long as it continues to be invoked, will not[m
[31m-// be triggered. The function will be called after it stops being called for[m
[31m-// N milliseconds. If `immediate` is passed, trigger the function on the[m
[31m-// leading edge, instead of the trailing.[m
[31m-// https://github.com/jashkenas/underscore/blob/1.6.0/underscore.js#L714[m
[31m-function debounce(func, wait, immediate) {[m
[31m-	var timeout, args, context, timestamp, result;[m
[31m-[m
[31m-	var later = function() {[m
[31m-		var last = +new Date() - timestamp;[m
[31m-		if (last < wait) {[m
[31m-			timeout = setTimeout(later, wait - last);[m
[31m-		}[m
[31m-		else {[m
[31m-			timeout = null;[m
[31m-			if (!immediate) {[m
[31m-				result = func.apply(context, args);[m
[31m-				context = args = null;[m
[31m-			}[m
[31m-		}[m
[31m-	};[m
[31m-[m
[31m-	return function() {[m
[31m-		context = this;[m
[31m-		args = arguments;[m
[31m-		timestamp = +new Date();[m
[31m-		var callNow = immediate && !timeout;[m
[31m-		if (!timeout) {[m
[31m-			timeout = setTimeout(later, wait);[m
[31m-		}[m
[31m-		if (callNow) {[m
[31m-			result = func.apply(context, args);[m
[31m-			context = args = null;[m
[31m-		}[m
[31m-		return result;[m
[31m-	};[m
[31m-}[m
[31m-[m
[31m-;;[m
