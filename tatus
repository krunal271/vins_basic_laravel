[31m-	/* Event Modification Math[m
[31m-	-----------------------------------------------------------------------------------------*/[m
[31m-[m
[31m-[m
[31m-	// Modifies an event and all related events by applying the given properties.[m
[31m-	// Special date-diffing logic is used for manipulation of dates.[m
[31m-	// If `props` does not contain start/end dates, the updated values are assumed to be the event's current start/end.[m
[31m-	// All date comparisons are done against the event's pristine _start and _end dates.[m
[31m-	// Returns an object with delta information and a function to undo all operations.[m
[31m-	// For making computations in a granularity greater than day/time, specify largeUnit.[m
[31m-	// NOTE: The given `newProps` might be mutated for normalization purposes.[m
[31m-	function mutateEvent(event, newProps, largeUnit) {[m
[31m-		var miscProps = {};[m
[31m-		var oldProps;[m
[31m-		var clearEnd;[m
[31m-		var startDelta;[m
[31m-		var endDelta;[m
[31m-		var durationDelta;[m
[31m-		var undoFunc;[m
[31m-[m
[31m-		// diffs the dates in the appropriate way, returning a duration[m
[31m-		function diffDates(date1, date0) { // date1 - date0[m
[31m-			if (largeUnit) {[m
[31m-				return diffByUnit(date1, date0, largeUnit);[m
[31m-			}[m
[31m-			else if (newProps.allDay) {[m
[31m-				return diffDay(date1, date0);[m
[31m-			}[m
[31m-			else {[m
[31m-				return diffDayTime(date1, date0);[m
[31m-			}[m
[31m-		}[m
[31m-[m
[31m-		newProps = newProps || {};[m
[31m-[m
[31m-		// normalize new date-related properties[m
[31m-		if (!newProps.start) {[m
[31m-			newProps.start = event.start.clone();[m
[31m-		}[m
[31m-		if (newProps.end === undefined) {[m
[31m-			newProps.end = event.end ? event.end.clone() : null;[m
[31m-		}[m
[31m-		if (newProps.allDay == null) { // is null or undefined?[m
[31m-			newProps.allDay = event.allDay;[m
[31m-		}[m
[31m-		normalizeEventDates(newProps);[m
[31m-[m
[31m-		// create normalized versions of the original props to compare against[m
[31m-		// need a real end value, for diffing[m
[31m-		oldProps = {[m
[31m-			start: event._start.clone(),[m
[31m-			end: event._end ? event._end.clone() : t.getDefaultEventEnd(event._allDay, event._start),[m
[31m-			allDay: newProps.allDay // normalize the dates in the same regard as the new properties[m
[31m-		};[m
[31m-		normalizeEventDates(oldProps);[m
[31m-[m
[31m-		// need to clear the end date if explicitly changed to null[m
[31m-		clearEnd = event._end !== null && newProps.end === null;[m
[31m-[m
[31m-		// compute the delta for moving the start date[m
[31m-		startDelta = diffDates(newProps.start, oldProps.start);[m
[31m-[m
[31m-		// compute the delta for moving the end date[m
[31m-		if (newProps.end) {[m
[31m-			endDelta = diffDates(newProps.end, oldProps.end);[m
[31m-			durationDelta = endDelta.subtract(startDelta);[m
[31m-		}[m
[31m-		else {[m
[31m-			durationDelta = null;[m
[31m-		}[m
[31m-[m
[31m-		// gather all non-date-related properties[m
[31m-		$.each(newProps, function(name, val) {[m
[31m-			if (isMiscEventPropName(name)) {[m
[31m-				if (val !== undefined) {[m
[31m-					miscProps[name] = val;[m
[31m-				}[m
[31m-			}[m
[31m-		});[m
[31m-[m
[31m-		// apply the operations to the event and all related events[m
[31m-		undoFunc = mutateEvents([m
[31m-			clientEvents(event._id), // get events with this ID[m
[31m-			clearEnd,[m
[31m-			newProps.allDay,[m
[31m-			startDelta,[m
[31m-			durationDelta,[m
[31m-			miscProps[m
[31m-		);[m
[31m-[m
