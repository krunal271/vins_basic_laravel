[31m-		}[m
[31m-[m
[31m-		if (this.scrollLeftVel < 0) { // scrolling left?[m
[31m-			if (el.scrollLeft() <= 0) { // already scrolled all the left?[m
[31m-				this.scrollLeftVel = 0;[m
[31m-			}[m
[31m-		}[m
[31m-		else if (this.scrollLeftVel > 0) { // scrolling right?[m
[31m-			if (el.scrollLeft() + el[0].clientWidth >= el[0].scrollWidth) { // already scrolled all the way right?[m
[31m-				this.scrollLeftVel = 0;[m
[31m-			}[m
[31m-		}[m
[31m-	},[m
[31m-[m
[31m-[m
[31m-	// This function gets called during every iteration of the scrolling animation loop[m
