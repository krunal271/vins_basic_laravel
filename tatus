[31m-			else {[m
[31m-				dropLocation = pluckEventDateProps(event);[m
[31m-			}[m
[31m-[m
[31m-			dropLocation.start.add(delta);[m
[31m-			if (dropLocation.end) {[m
[31m-				dropLocation.end.add(delta);[m
[31m-			}[m
[31m-		}[m
[31m-		else {[m
[31m-			// if switching from day <-> timed, start should be reset to the dropped date, and the end cleared[m
