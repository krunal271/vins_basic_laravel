[31m-	// A falsy returned value indicates an invalid drop.[m
[31m-	// DOES NOT consider overlap/constraint.[m
[31m-	computeEventDrop: function(startSpan, endSpan, event) {[m
[31m-		var calendar = this.view.calendar;[m
[31m-		var dragStart = startSpan.start;[m
[31m-		var dragEnd = endSpan.start;[m
