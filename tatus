[31m-/*[m
[31m-Options binding/triggering system.[m
[31m-*/[m
[31m-Calendar.mixin({[m
[31m-[m
[31m-	// A map of option names to arrays of handler objects. Initialized to {} in Calendar.[m
[31m-	// Format for a handler object:[m
[31m-	// {[m
[31m-	//   func // callback function to be called upon change[m
[31m-	//   names // option names whose values should be given to func[m
[31m-	// }[m
[31m-	optionHandlers: null, [m
[31m-[m
[31m-	// Calls handlerFunc immediately, and when the given option has changed.[m
[31m-	// handlerFunc will be given the option value.[m
[31m-	bindOption: function(optionName, handlerFunc) {[m
[31m-		this.bindOptions([ optionName ], handlerFunc);[m
[31m-	},[m
[31m-[m
[31m-	// Calls handlerFunc immediately, and when any of the given options change.[m
[31m-	// handlerFunc will be given each option value as ordered function arguments.[m
[31m-	bindOptions: function(optionNames, handlerFunc) {[m
[31m-		var handlerObj = { func: handlerFunc, names: optionNames };[m
[31m-		var i;[m
[31m-[m
[31m-		for (i = 0; i < optionNames.length; i++) {[m
[31m-			this.registerOptionHandlerObj(optionNames[i], handlerObj);[m
[31m-		}[m
[31m-[m
[31m-		this.triggerOptionHandlerObj(handlerObj);[m
[31m-	},[m
[31m-[m
[31m-	// Puts the given handler object into the internal hash[m
[31m-	registerOptionHandlerObj: function(optionName, handlerObj) {[m
[31m-		(this.optionHandlers[optionName] || (this.optionHandlers[optionName] = []))[m
[31m-			.push(handlerObj);[m
[31m-	},[m
[31m-[m
[31m-	// Reports that the given option has changed, and calls all appropriate handlers.[m
[31m-	triggerOptionHandlers: function(optionName) {[m
[31m-		var handlerObjs = this.optionHandlers[optionName] || [];[m
[31m-		var i;[m
[31m-[m
[31m-		for (i = 0; i < handlerObjs.length; i++) {[m
[31m-			this.triggerOptionHandlerObj(handlerObjs[i]);[m
[31m-		}[m
[31m-	},[m
[31m-[m
[31m-	// Calls the callback for a specific handler object, passing in the appropriate arguments.[m
[31m-	triggerOptionHandlerObj: function(handlerObj) {[m
[31m-		var optionNames = handlerObj.names;[m
[31m-		var optionValues = [];[m
[31m-		var i;[m
[31m-[m
[31m-		for (i = 0; i < optionNames.length; i++) {[m
[31m-			optionValues.push(this.options[optionNames[i]]);[m
[31m-		}[m
[31m-[m
[31m-		handlerObj.func.apply(this, optionValues); // maintain the Calendar's `this` context[m
[31m-	}[m
[31m-[m
[31m-});[m
