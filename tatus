[31m-			if (dateDelta) {[m
[31m-				newProps.start.add(dateDelta);[m
[31m-				if (newProps.end) {[m
[31m-					newProps.end.add(dateDelta);[m
[31m-				}[m
[31m-			}[m
[31m-[m
[31m-			if (durationDelta) {[m
[31m-				newProps.end.add(durationDelta); // end already ensured above[m
[31m-			}[m
[31m-[m
[31m-			// if the dates have changed, and we know it is impossible to recompute the[m
[31m-			// timezone offsets, strip the zone.[m
