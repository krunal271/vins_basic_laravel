[31m-				topVel = topCloseness * this.scrollSpeed * -1; // negative. for scrolling up[m
[31m-			}[m
[31m-			else if (bottomCloseness >= 0 && bottomCloseness <= 1) {[m
[31m-				topVel = bottomCloseness * this.scrollSpeed;[m
[31m-			}[m
[31m-[m
[31m-			// translate horizontal closeness into velocity[m
[31m-			if (leftCloseness >= 0 && leftCloseness <= 1) {[m
