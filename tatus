[31m-				}[m
[31m-			}[m
[31m-			else {[m
[31m-				events.push(abstractEvent); // return the original event. will be a one-item array[m
[31m-			}[m
[31m-		}[m
[31m-[m
[31m-		return events;[m
[31m-	}[m
